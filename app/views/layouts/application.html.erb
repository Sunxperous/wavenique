<!DOCTYPE html>
<html>
<head>
  <title>Vagrant</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

	<%= render 'shared/nav' %>
<%= yield %>
<% if Rails.env.development? %>
	<%= yield :debug %>
	<%= debug params %>
	<%= debug session %>
	<%= debug current_user %>
<% end %>

</body>
</html>
